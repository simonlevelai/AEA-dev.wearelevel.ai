{
  "name": "ask-eve-assist",
  "version": "1.0.0",
  "description": "Ask Eve Assist - AI chatbot for gynaecological health information from The Eve Appeal",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "dev:ultra-cheap": "DEPLOYMENT_TYPE=ULTRA_CHEAP nodemon --exec ts-node src/index.ts",
    "dev:multiagent": "nodemon --exec ts-node src/index-multiagent.ts",
    "dev:bot": "nodemon --exec ts-node src/bot/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:multiagent": "node dist/index-multiagent.js",
    "start:bot": "node dist/bot/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:safety": "jest --testPathPattern=safety --verbose",
    "test:bot": "jest --testPathPattern=bot --verbose",
    "test:integration": "jest --testPathPattern=Microsoft365AgentsIntegration --verbose",
    "test:ultra-cheap-architecture": "jest --testPathPattern=UltraCheapArchitecture --verbose",
    "test:azure-services-factory": "jest --testPathPattern=AzureServicesFactory --verbose", 
    "test:cost-optimization": "jest --testPathPattern=CostOptimization --verbose",
    "test:azure-integration": "jest --testPathPattern=AzureIntegration --verbose",
    "verify:cost-target-achieved": "ts-node scripts/verify-ultra-cheap-success.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "setup:ultra-cheap-search": "ts-node scripts/setup-azure-ai-search-free.ts",
    "setup:ultra-cheap-table-storage": "ts-node scripts/setup-azure-table-storage.ts",
    "upload:content-free-tier": "ts-node scripts/upload-content-ultra-cheap.ts",
    "monitor:costs": "ts-node scripts/monitor-ultra-cheap-costs.ts",
    "deploy:ultra-cheap": "ts-node scripts/setup-ultra-cheap-azure.ts",
    "generate:cost-analysis-report": "ts-node scripts/generate-cost-analysis-report.ts",
    "crawl": "ts-node scripts/crawl-website.ts",
    "ingest": "ts-node scripts/ingest-content.ts",
    "setup-index": "ts-node scripts/setup-search-index.ts",
    "validate-sources": "ts-node scripts/validate-sources.ts",
    "process-pif": "ts-node scripts/simple-pif-processor.ts",
    "crawl-comprehensive": "ts-node scripts/comprehensive-crawler.ts"
  },
  "keywords": [
    "health",
    "chatbot",
    "eve-appeal",
    "azure",
    "typescript"
  ],
  "author": "Level AI",
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^4.0.0",
    "@azure/openai": "^2.0.0",
    "@azure/search-documents": "^12.1.0",
    "@azure/storage-blob": "^12.17.0",
    "@azure/data-tables": "^13.2.2",
    "@azure/arm-search": "^3.2.0",
    "@azure/arm-storage": "^18.1.0",
    "@azure/arm-monitor": "^8.0.1",
    "@azure/monitor-query": "^1.1.1",
    "@microsoft/agents-hosting": "^2.0.0",
    "@microsoft/agents-hosting-express": "^2.0.0",
    "@microsoft/agents-orchestration": "^1.0.0",
    "@microsoft/copilot-studio-sdk": "^1.0.0",
    "@supabase/supabase-js": "^2.53.0",
    "@types/helmet": "^0.0.48",
    "applicationinsights": "^2.9.0",
    "axios": "^1.6.0",
    "botbuilder": "^4.17.0-blobs1",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.0",
    "helmet": "^8.1.0",
    "openai": "^4.104.0",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.15.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.9",
    "@types/pdf-parse": "^1.1.0",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
